<div id="service-index">
  <h1>Lawgix OGX Services</h1>
  <table>
    <thead>
    <tr>
      <td>Lease Number</td>
      <td>Lessee Last Name</td>
      <td>County</td>
      <td>Workflows</td>
    </tr>
    </thead>
    <tbody>
    <% @services.each do |service| %>
        <tr>
          <td><%= lease_number %></td>
          <td><%= lessee_last_name %></td>
          <td><%= state %></td>
          <td><%= county %></td>
          <td><%= service.flows.count %></td>
        </tr>
    <% end %>
    </tbody>
  </table>
</div>
