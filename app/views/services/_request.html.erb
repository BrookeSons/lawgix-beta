<div class="tab-content">
    <%= f.label :state %>
    <%= f.select :state, collection: ['Ohio','West Virginia'] %>
    <%= f.label :lease_number %>
    <%= f.text_field :lease_number %>
    <%= f.label :description %>
    <%= f.text_area :description %>
    <%= f.check_box(:serial_flows, {:multiple => true}, 'Opinion', nil) %>
    <%= f.label 'Abstract' %>
    <%= f.check_box(:serial_flows, {:multiple => true}, 'Abstract', nil) %>
    <%= f.label 'Opinion' %>
    <%= f.check_box(:serial_flows, {:multiple => true}, 'Curative', nil) %>
    <%= f.label 'Curative' %>
  <div id="deliv-date">
    <%= f.label 'Delivery Date' %>
    <%= f.date_field :requested_delivery %>
  </div>
</div>
