<table>
  <thead>
  <tr>
    <td>Lease</td>
    <td>Abstractor</td>
  </tr>
  </thead>
  <tbody>

  <% @abs = @services.in_state('assigned') %>
  <%= will_paginate @services %>
  <% @abs.each do |service| %>
      <tr>
        <td><%= link_to service.lease_number, service %></td>
        <%  service.users.each do |user| %>
        <td><%= link_to user.last_name, user %></td>
       <% end %>
      </tr>
  <% end %>
  </tbody>
</table>
