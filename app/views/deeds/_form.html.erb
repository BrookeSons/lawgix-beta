<%= simple_form_for(@deed) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :db %>
    <%= f.input :db_page %>
    <%= f.input :d_date %>
    <%= f.input :grantor %>
    <%= f.input :grantee %>
    <%= f.input :d_type %>
    <%= f.input :comments %>
    <%= f.input :d_image %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
