<table>
  <thead>
  <tr>
    <th>Name</th>
    <th>Phone</th>
    <th>Type</th>
    <ul>
        <li></li>
        <li></li>
    </ul></tr>
  </thead>
  <tbody>
  <% @contacts = @project.contacts %>
  <% @contacts.each do |contact| %>
      <tr>
          <td><%= link_to contact.name, contact %></td>
          <td><%= contact.bus_phone  %>
          <td><%= contact.type %></td> 
        <% end %>
    </tr>
  </tbody>
</table>
