<div id="parcel-index">
  <table>
    <thead>
    <tr>
      <th>Parcel ID</th>
      <th>County</th>
      <th>Township</th>
      <th>Surface Owner</th>
      <th>Lease</th>
    </tr>
    </thead>
    <tbody>
    <% @project.parcels.each do |parcel| %>
        <tr>
          <td><%= link_to parcel.tax_parcel, parcel %></td>
          <td><%= parcel.county %></td>
          <td><%= parcel.tshp %></td>
          <% if parcel.owners   %>
              <% parcel.owners.each do |owner| %>
                <td>  <%= link_to owner.name, owner  %> </td>
              <%  end %>
              <% end  %>
          <% if parcel.lease %>
          <td><%= link_to parcel.lease.lease_number, parcel.lease  %></td>
          <% end %>
        </tr>
    <% end %>
    </tbody>
  </table>
</div>
