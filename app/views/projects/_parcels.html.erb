<div id="parcel-index">
  <table>
    <thead>
    <tr>
      <td>Parcel ID</td>
      <td>County</td>
      <td>Lease</td>
    </tr>
    </thead>
    <tbody>
    <% @project.parcels.each do |parcel| %>
        <tr>
          <td><%= link_to parcel.tax_parcel, parcel %></td>
          <td><%= parcel.county %></td>
          <% if parcel.lease %>
          <td><%= link_to parcel.lease.lease_number, parcel.lease  %></td>
          <% end %>
        </tr>
    <% end %>
    </tbody>
  </table>
</div>
