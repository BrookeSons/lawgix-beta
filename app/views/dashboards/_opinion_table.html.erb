<table>
  <thead>
  <tr>
    <td>Lease Number</td>
    <td>Description</td>
    <td>Lessee<td>
    <td>Status</td>
    <td>Delivery Date</td>
    <td>State</td>
    <td>County</td>
    <td>Workflows</td>
  </tr>
  </thead>
  <tbody>
  <%= will_paginate @services %>
  <% @services.each do |service| %>
      <tr>
        <td><%= link_to service.lease_number, service %></td>
        <td><%= service.description %></td>
        <td><%= service.lessees.count %><td>
        <td><%= if service.current_state; service.current_state; end %></td>
        <td><%= service.requested_delivery %><td>
        <td><%= service.state %></td>
        <td><%= service.county %></td>
        <td><%= service.flows.count %></td>
        <td><%=  %></td>
      </tr>
  <% end %>
  </tbody>
</table>
