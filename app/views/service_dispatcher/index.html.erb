<div id="service-dashboard">

  <div id="new-requests" >
    <h2>Submitted Requests</h2>
    <%= render partial: 'new_table' %>
  </div>

  <div id="pre-dispatch">
    <h2>Fee Accepted</h2>
    <%= render 'pre_table' %>
  </div>

  <div id="assigned-requests">
    <h2>Assigned Requests</h2>
    <div id="abstract-table">
      <h4><%= link_to 'Abstracts', title_abstracts_path %> </h4>
      <%= render 'abstract_table' %>
    </div>

    <div id="opinion-table">
      <h4>Opinions</h4>
      <%= render 'opinion_table' %>
    </div>

    <div id="curative-table">
      <h4>Curative</h4>
      <%= render 'curative_table' %>
    </div>
  </div>
</div>
